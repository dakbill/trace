<div class="row">
  <fieldset>
		<legend>New Dataset</legend>
		<%= raw(flash[:message]) %>
		<%= form_for @data_set ,{url: save_uploaded_data_path } do |data_set|%>
			<div class="form-group">
				<label>Name</label>
				<%= data_set.text_field :name, :class => 'form-control' %>
				
			</div>
			<div class="form-group">
				<label>File</label>
				<%= data_set.file_field :data_set_file %>
			</div>
			<button type="submit" class="btn btn-default">Submit</button>
		<% end %>
	</fieldset>
</div>
