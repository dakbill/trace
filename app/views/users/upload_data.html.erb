<div class="row">
  <fieldset>
		<%= raw(flash[:message]) %>
		<legend>New Dataset</legend>
		<%= form_for @data_set ,url: sign_in_path  do |data_set|%>
			<div class="form-group">
				<label>Name</label>
				<%= data_set.text_field :name, :class => 'form-control' %>
				
			</div>
			<div class="form-group">
				<label>File</label>
				<input type="file"  />
				
			</div>
			<button type="submit" class="btn btn-default">Submit</button>
		<% end %>
	</fieldset>
</div>
